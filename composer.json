{
    "name": "northernestateagencies/reapit-foundations",
    "description": "A very simple wrapper for Reapit Foundations.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "NorthernEstateAgencies\\ReapitFoundations\\": "src/",
            "NorthernEstateAgencies\\ReapitFoundations\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NorthernEstateAgencies\\ReapitFoundations\\Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Jae Toole",
            "email": "jae.toole@northernestateagencies.co.uk"
        }
    ],
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "NorthernEstateAgencies\\ReapitFoundations\\ReapitFoundationsServiceProvider"
            ]
        }
    }
}
